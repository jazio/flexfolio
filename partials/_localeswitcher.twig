<ul class="navbar-nav mr-auto">
    {% for locale in locales|default([]) %}
    <li {% if locale.get('active') %} class="nav-item active" style="display: none;" {% else %} class="nav-item" {% endif %}>
        <a class="nav-link" style="color: red" title="{{ locale.get('label') }}" href="{{ locale.get('url') }}">
            {{ locale.get('label') }}
        </a>
    {% endfor %}
</ul>